{
  "hosting": [
    {
      "target": "kekkonbu",
      "source": "apps/kekkonbu",
      "frameworksBackend": {
        "region": "asia-northeast1"
      }
    },
    {
      "target": "sakeflow",
      "source": "apps/sakeflow",
      "frameworksBackend": {
        "region": "asia-northeast1"
      }
    }
  ],
  "apphosting": [
    {
      "backendId": "kekkonbu-backend",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    },
    {
      "backendId": "sakeflow-backend",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ]
}
